{
  "name": "starter-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "vue": "^3.1.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.1.5",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.14.0",
    "postcss": "^8.3.6",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.2.6",
    "vite": "^2.4.3",
    "vite-plugin-components": "^0.11.5",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-svg-icons": "^1.0.1"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none",
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "ignore"
  },
  "eslint": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "rules": {
      "vue/no-unused-vars": "error"
    }
  }
}
